
# Initializing some arrays of elements
Array1 = [1, 2, 3, 4]
Array2 = ["a", "b", "c"]
Array3 = ["gfg", "Geeks", "GeeksforGeeks"]
   
# Calling push() function
A = Array1.push(5, 6, 7)                
B = Array2.push("d", "e", "f")               
C = Array3.push("Geek")  
   
# Printing the array of pushed element
puts "#{A}"
puts "#{B}"
puts "#{C}"
